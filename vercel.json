{
  "version": 2,
  "builds": [
    {
      "src": "src/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/src/website/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "src"
  }
}
